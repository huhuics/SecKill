<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
<generatorConfiguration> 
	
	<!-- 配置文件路径 -->
	<properties resource="config/config.properties"/>

    <context id="default"  targetRuntime="MyBatis3" defaultModelType="flat">
        <commentGenerator type="cn.szfesc.mpay2.mybatis.CommentGenerator">
            <property name="suppressDate" value="true"/>  
            <!-- 是否去除自动生成的注释 -->
            <property name="suppressAllComments" value="true"/>  
            <property name="addRemarkComments" value="true"/>  
            <property name="javaFileEncoding" value="UTF-8"/>     
        </commentGenerator>  
        <!-- 数据库连接信息 -->
        <jdbcConnection driverClass="${driver}" connectionURL="${url}" userId="${username}" password="${password}"/>  
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>  
        <!-- 生成领域对象的包名和位置 -->
        <javaModelGenerator targetPackage="${domain.package}" targetProject="src/main/java">  
            <property name="enableSubPackages" value="true"/>  
            <property name="trimStrings" value="true"/>  
        </javaModelGenerator>  
        <!-- 生成mapping文件的包名和位置 -->
        <sqlMapGenerator targetPackage="mapping" targetProject="src/main/resources">  
            <property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>  
        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="${dao.package}" targetProject="src/main/java">  
            <property name="enableSubPackages" value="true"/>  
        </javaClientGenerator>  
		
		<!-- 生成支付宝交易表表 
		<table tableName="ORD_ALIPAY_PAY" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
			<columnOverride column="TOTAL_AMOUNT" javaType="cn.fesc.facade.common.util.Money"></columnOverride>
			<columnOverride column="DISCOUNTABLE_AMOUNT" javaType="cn.fesc.facade.common.util.Money"></columnOverride>
			<columnOverride column="UNDISCOUNTABLE_AMOUNT" javaType="cn.fesc.facade.common.util.Money"></columnOverride>
			<columnOverride column="RECEIPT_AMOUNT" javaType="cn.fesc.facade.common.util.Money"></columnOverride>
			<columnOverride column="REFUND_AMOUNT" javaType="cn.fesc.facade.common.util.Money"></columnOverride>
		</table>
		-->
		
		<table tableName="MER_PAY_ORG" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
		</table>
    </context>  
</generatorConfiguration>